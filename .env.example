# Gemini API key from Google AI Studio (https://makersuite.google.com/)
GEMINI_API_KEY=your_gemini_api_key_here

# Google Maps API Keys for iOS and Android
GOOGLE_MAPS_API_KEY_IOS=your_ios_maps_api_key_here
